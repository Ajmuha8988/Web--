@{
    Layout = "";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Главное меню</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Book_NET9.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/lib/bootstrap-icons-1.11.3/font/bootstrap-icons.min.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-9ndCyUaIbzAi2FUVXJi0CjmCapSmO7SnpJef0486qhLnuZ2cdeRhO02iuK6FUUVM" crossorigin="anonymous">
</head>
<body class="wrapgrid containerstudent w-100 bg-light  Pollock ">
    <button type="button" class="btn btn-light btn-sm" style="display:none" id="buttonback" onclick="backcontent()">
        <span class="bi bi-caret-left"></span>
    </button>
    <main class="d-flex off-container1lvl">
        <aside style="width: 30%; height: 100%" class="bg-light border-1 border-end border-start border-bottom off-container2lvl" id="navbarquestions">
            @*Список*@
            @*Поисковик*@
            <button type="button" class="btn m-1 butGray" onclick="toggleVisibility1()" style="width: 97%; ">
                Структура программного обеспечения ПК
            </button>
            <button type="button" class="btn m-1 butGray " onclick="toggleVisibility2()" style="width: 97%">
                Системное ПО
            </button>
            <button type="button" class="btn m-1 butGray" onclick="toggleVisibility3()" style="width: 97%">
                Прикладное и инструментальное ПО
            </button>
            <button type="button" class="btn m-1 butGray" onclick="toggleVisibility4()" style="width: 97%">
                Файловые системы
            </button>
            <button type="button" class="btn m-1 butGray" onclick="toggleVisibility5()" style="width: 97%">
                Классификация операционных систем
            </button>
            <button type="button" class="btn m-1 butGray" onclick="toggleVisibility6()" style="width: 97%">
                Операционная система Windows
            </button>
            <button type="button" class="btn m-1 butGray" onclick="toggleVisibility7()" style="width: 97%">
                Изучение меню и интерфейса программы Microsoft Access
            </button>
            <button type="button" class="btn m-1 butGray" onclick="toggleVisibility8()" style="width: 97%">
                Проектирование базы данных и создание таблиц
            </button>
            <button type="button" class="btn m-1 butGray" onclick="toggleVisibility9()" style="width: 97%">
                Создание таблиц и ввод исходных данных
            </button>
            <button type="button" class="btn m-1 butGray" onclick="toggleVisibility10()" style="width: 97%">
                Мастер таблиц, импорт таблиц. Режим таблицы, режим «Коструктор»
            </button>
            <button type="button" class="btn m-1 butGray" onclick="toggleVisibility11()" style="width: 97%">
                Модификация структуры табличного файла
            </button>
            <button type="button" class="btn m-1 butGray" onclick="toggleVisibility12()" style="width: 97%">
                Определение полей. Свойства полей таблицы.
            </button>
            <button type="button" class="btn m-1 butGray" onclick="toggleVisibility13()" style="width: 97%">
                Применение ключевых полей. Установка связей между таблицами
            </button>
            <button type="button" class="btn m-1 butGray" onclick="toggleVisibility14()" style="width: 97%">
                Создание форм для наполнения нескольких таблиц
            </button>
            <button type="button" class="btn m-1 butGray" onclick="toggleVisibility15()" style="width: 97%">
                Наполнение БД данными с использованием построенных форм
            </button>
            <button type="button" class="btn m-1 butGray" onclick="toggleVisibility16()" style="width: 97%">
                Входной контроль. Основы работы с БД.
            </button>
        </aside>
        <main style="width: 70%; height: 100%;" class="bg-light border-1 border-end off-container3lvl" id="mainpanel">
            <div id="content1" style="display:none" class="row justify-content-center">
                <form asp-action="TestView1" asp-controller="StudentView">
                    <iframe src="~/html/students/1_page.html" style="height: 600px; border: none; width:100%" class="container" id="L1"></iframe>
                    <div style="height: 25px"></div>
                    <div class="text-center">
                        <button class="btn btn-dark Pollock w-50">Пройти тестирование</button>
                    </div>
                </form>
            </div>
            <div id="content2" style="display:none">
                 <form asp-action="TestView1" asp-controller="StudentView">
                    <iframe src="~/html/students/2_page.html" style=" height: 600px; border: none; width:100%" id="L2"></iframe>
                    <div style="height: 25px"></div>
                    <div class="text-center">
                        <button class="btn btn-dark Pollock w-50">Пройти тестирование</button>
                    </div>
                 </form>
            </div>
            <div id="content3" style="display:none">
                <form asp-action="TestView1" asp-controller="StudentView">
                    <iframe src="~/html/students/3_page.html" style=" height: 600px; border: none; width:100%" id="L3"></iframe>
                    <div style="height: 25px"></div>
                    <div class="text-center">
                        <button class="btn btn-dark Pollock w-50">Пройти тестирование</button>
                    </div>
                </form>
            </div>
            <div id="content4" style="display:none">
                <form asp-action="TestView1" asp-controller="StudentView">
                    <iframe src="~/html/students/4_page.html" style="height: 600px; border: none; width:100%" id="L4"></iframe>
                    <div style="height: 25px"></div>
                    <div class="text-center">
                        <button class="btn btn-dark Pollock w-50">Пройти тестирование</button>
                    </div>
                </form>
            </div>
            <div id="content5" style="display:none">
                <form asp-action="TestView1" asp-controller="StudentView">
                    <iframe src="~/html/students/5_page.html" style="height: 600px; border: none; width:100%" id="L5"></iframe>
                    <div style="height: 25px"></div>
                    <div class="text-center">
                        <button class="btn btn-dark Pollock w-50">Пройти тестирование</button>
                    </div>
                </form>
            </div>
            <div id="content6" style="display:none">
                <form asp-action="TestView1" asp-controller="StudentView">
                    <iframe src="~/html/students/6_page.html" style="height: 600px; border: none; width:100%" id="L6"></iframe>
                    <div style="height: 25px"></div>
                    <div class="text-center">
                        <button class="btn btn-dark Pollock w-50">Пройти тестирование</button>
                    </div>
                </form>
            </div>
        </main>
    </main>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
</body>
</html>
